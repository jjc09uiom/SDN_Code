orm.value.switch }\r\n          }).then(res => {\r\n          // await addlink(form.value)\r\n            ElMessage({\r\n              message: '提交成功',\r\n              type: 'success'\r\n            })\r\n          })\r\n        })\r\n      .catch(() => {\r\n        ElMessage({\r\n            type: 'info',\r\n            message: '取消添加'\r\n          })\r\n      })\r\n    } else {\r\n      console.log('error submit!')\r\n      return false\r\n    }\r\n  })\r\n}\r\nconst rules = ref({\r\n  switch: [\r\n    {\r\n      required: true,\r\n      message: '不能为空',\r\n      trigger: 'blur'\r\n    }\r\n  ]\r\n})\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.el-input{\r\n  width: 50% !important;\r\n}\r\n</style>\r\n"]}]}