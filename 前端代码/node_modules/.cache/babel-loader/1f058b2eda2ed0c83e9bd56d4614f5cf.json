ancelButtonText: '取消',\r\n          type: 'warning'\r\n        })\r\n      .then(() => {\r\n          // await addflow(textarea.value)\r\n          axios({\r\n            method: 'post',\r\n            url: '/addflow',\r\n            data: { text: textarea.value }\r\n          }).then(res => {\r\n            console.log(1234)\r\n            ElMessage({\r\n              message: '提交成功',\r\n              type: 'success'\r\n            })\r\n          })\r\n        })\r\n      .catch(() => {\r\n        ElMessage({\r\n            type: 'info',\r\n            message: '取消提交'\r\n          })\r\n      })\r\n    } else {\r\n      console.log('error submit!')\r\n      return false\r\n    }\r\n  })\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .text-area textarea {\r\n    width: 50%;\r\n    height: 50%;\r\n    margin-top: 50rpx;\r\n  }\r\n  .el-button{\r\n    margin-top: 20px;\r\n  }\r\n</style>\r\n"]}]}